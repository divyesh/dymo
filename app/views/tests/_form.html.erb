<%= simple_form_for(@test) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :loinc %>
    <%= f.association :test_group, prompt: 'Select Test Group' %> <div style="padding-left: 500px; padding-top: 8px;"><%= link_to "Add new Test Group", new_test_group_path %></div><div class="clear"></div><br />
    <%= f.input :test_code %>
    <%= f.input :test_name %>
    <%= f.input :specimen_source %>
    <%= f.input :position %>
    <%= f.input :visible_in_list %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
