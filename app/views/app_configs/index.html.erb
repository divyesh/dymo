<h1>Listing App configs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_configs.each do |app_config| %>
      <tr>
        <td><%= app_config.name %></td>
        <td><%= app_config.value %></td>
        <td><%= link_to 'Show', app_config %></td>
        <td><%= link_to 'Edit', edit_app_config_path(app_config) %></td>
        <td><%= link_to 'Destroy', app_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App config', new_app_config_path %>
